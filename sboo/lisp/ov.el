(goto-char (ov-end (ov-next <span class="pl-c1">nil</span> 'bbb <span class="pl-c1">t</span>)))

(ov-next 'aaa)
(ov-next 'aaa <span class="pl-c1">t</span>)
(ov-next <span class="pl-c1">300</span> 'aaa)
(ov-next (point) 'aaa <span class="pl-c1">t</span>)</pre></div>
